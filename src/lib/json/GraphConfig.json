{
  "restURL": "http://localhost:33381",
  "language": "en",
  "debugMode":
  {
    "enabled": false,
    "connectorLabelText": 10
  },
  "layoutPadding":
  {
    "x": 30,
    "y": 30
  },
  "longPressTime": 1.5,
  "logUpdateTime": 60,
  "viewDefault": {"x": 0, "y": 0, "w": 5000, "h": 5000},
  "zoomFactor": 1.1,
  "connector":
  {
    "size": 5,
    "padding": 10
  },
  "websocket":
  {
    "resizeProps":
    {
      "aspectRatio": 1,
      "minWidth": 150,
      "minHeight": 150
    },
    "defaultProps":
    {
      "size": { "x": 400, "y": 400 },
      "beams": false,
      "projection": true,
      "beam_multiplier": 10,
      "points": false,
      "ip": "ws://localhost",
      "port": 33382
    }
  },
  "fontSize":
  {
    "connectorLabel": 12,
    "propertyLabel": 10,
    "nodeTitle": 12,
    "propertySettings": 10,
    "selectorLabel": 10,
    "iconLabel": 12
  },
  "node":
  {
    "iconSize": 20
  },
  "edge":
  {
    "hoverTime": 2
  },
  "infoPanel":
  {
    "iconSize": 30
  },
  "menu":
  {
    "size": 5,
    "hoverTime": 0.5,
    "pinnedPosition":
    {
      "x": 300,
      "y": 0
    },
    "parents": ["core", "trigger", "maths", "audio", "vector", "colour",
                "midi", "laser", "dmx", "time-series", "iot"],
    "children":
    {
      "core": ["clone",
        "clone-info", "compare", "filter", "graph",
        "interpolator", "log", "oscillator", "pin",
        "random", "switch", "timer"],
      "trigger": ["beat", "count", "memory", "pin", "selector", "start",
               "latch"],
      "maths": ["add", "subtract", "multiply", "divide", "mod", "power",
        "sin", "cos", "tan", "asin", "acos", "atan",
        "log10", "log", "exp10", "exp", "sqrt", "square", "cube",
        "inverse", "floor", "round", "abs", "limit", "wrap", "polar-position",
        "integrate", "differentiate" ],
      "audio": ["level", "oscillator", "filter",
                "bit-crush", "delay", "envelope", "loop",
                "pitch-shift", "position", "reverb", "wav-in", "alsa-in",
                "alsa-out","sdl-in", "sdl-out"],
      "vector": ["clip", "fade", "figure", "graph", "oscilloscope", "pin",
                 "rotate", "scale", "stroke", "pattern", "svg", "switch",
                 "translate", "websocket-display"],
      "colour": ["blend", "hsl", "pin", "rgb", "switch", "split"],
      "laser": ["add-blanking-anchors", "add-vertex-repeats", "beamify",
                "idn-out", "infill-lines", "reorder-segments", "show-blanking"],
      "dmx": ["set-value", "get-value", "bitmap-render",

      "midi": ["alsa-in", "control-in", "keyboard-in", "key-in", "pin"],
      "time-series": ["average", "last", "offset", "plot",
                      "scale", "web-fetch"],
      "iot": ["load-dscusb", "mqtt-in"]
    }
  },
  "interpolate":
  {
    "resizeProps":
    {
      "aspectRatio": 0,
      "minWidth": 120,
      "minHeight": 120
    }
  }

{
  "audio/sdl-out":
  {
    "description": "Audio output for SDL",
    "width": 100,
    "height": 50,
    "properties":
    {
      "buffer-size":
      {
        "description": "Buffer size in samples (must be a power of 2)"
      },
      "channels":
      {
        "description": "Number of channels",
        "controlType": "none/none",
        "rangeMin": 0,
        "rangeMax": 2
      },
      "device":
      {
        "description": "Device output to"
      },
      "max-delay":
      {
        "description": "Maximum delay (in samples) before dropping data"
      },
      "max-recovery":
      {
        "description": "Maximum samples to drop to recover delay"
      }
    }
  },
  "core/beat":
  {
    "description": "Regular beat pulse",
    "width": 100,
    "height": 90,
    "properties":
    {
      "interval":
      {
        "description": "Interval in seconds",
        "controlType": "knob/log",
        "rangeMin": 0.001,
        "rangeMax": 1000,
        "increment": 0.001,
        "x": 25,
        "y": 22
      },
      "offset":
      {
        "description": "Offset",
        "controlType": "knob/default",
        "rangeMin": 0,
        "rangeMax": 1,
        "increment": 0.1,
        "x": 65,
        "y": 22
      },
      "start":
      {
        "description": "Start",
        "controlType": "button/default",
        "x": 32,
        "y": 60
      },
      "stop":
      {
        "description": "Stop",
        "controlType": "button/default",
        "x": 72,
        "y": 60
      }
    }
  },
  "core/envelope":
  {
    "description": "Provide an ADSR envelope on trigger/release",
    "width": 190,
    "height": 90,
    "properties":
    {
      "attack":
      {
        "description": "Attack time",
        "controlType": "knob/default",
        "rangeMin": 0,
        "rangeMax": 10,
        "increment": 0.001,
        "x": 30,
        "y": 30
      },
      "decay":
      {
        "description": "Decay time",
        "controlType": "knob/default",
        "rangeMin": 0,
        "rangeMax": 10,
        "increment": 0.05,
        "x": 70,
        "y": 30
      },
      "release":
      {
        "description": "Release time",
        "controlType": "knob/log",
        "rangeMin": 0.01,
        "rangeMax": 30,
        "increment": 0.1,
        "x": 150,
        "y": 30
      },
      "reset":
      {
        "description": "Reset",
        "controlType": "button/default",
        "x": 82,
        "y": 65
      },
      "start":
      {
        "description": "Start",
        "controlType": "button/default",
        "x": 32,
        "y": 65
      },
      "stop":
      {
        "description": "Stop",
        "controlType": "button/default",
        "x": 57,
        "y": 65
      },
      "sustain":
      {
        "description": "Sustain level",
        "controlType": "knob/default",
        "rangeMin": 0,
        "rangeMax": 1,
        "increment": 0.01,
        "x": 110,
        "y": 30
      }
    }
  },
  "core/multiply":
  {
    "description": "Multiply a control value",
    "width": 70,
    "height": 70,
    "properties":
    {
      "factor":
      {
        "description": "Factor to multiply with",
        "controlType": "knob/log",
        "rangeMin": 0.001,
        "rangeMax": 1000,
        "increment": 0.1,
        "x": 30,
        "y": 30
      },
      "input":
      {
        "description": "Base value",
        "controlType": "none/none"
      }
    }
  },
  "core/oscillator":
  {
    "description": "Oscillator",
    "width": 170,
    "height": 95,
    "properties":
    {
      "freq":
      {
        "description": "Frequency",
        "controlType": "knob/default",

        "rangeMax": 10,
        "increment": 0.1,
        "x": 25,
        "y": 22
      },
      "pulse-width":
      {
        "description": "Pulse Width",
        "controlType": "knob/default",
        "rangeMin": 0,
        "rangeMax": 10,
        "increment": 0.1,
        "x": 65,
        "y": 22
      },
      "wave":
      {
        "description": "Waveform",
        "controlType": "selector/vert",
        "available": ["sin","square","triangle","saw","random"],
        "x": 110,
        "y": 20
      },
      "start":
      {
        "description": "Start",
        "controlType": "button/default",
        "x": 32,
        "y": 60
      },
      "stop":
      {
        "description": "Stop",
        "controlType": "button/default",
        "x": 72,
        "y": 60
      }
    }
  },
